<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE geothm SYSTEM "geothm.dtd">
<geothm name="Wernick 003 (A,B,Mc --| C)">
	<!-- Wernick 003                                                    -->
	<!-- Let ABC be a triangle and Mc is midpoint of edge AB. Consider  -->
	<!-- RC-constructibility of vertex C from given points A, B and Mc. -->
	<constructions>
		<pfree label="A"/>
		<pfree label="B"/>
		<pfree label="C"/>
		<pmid label="Mc" point1="A" point2="B" />
	</constructions>
	<freepts>
		<point label="A" />
		<point label="B" />
		<point label="Mc" />
	</freepts>
	<conspts>
		<point label="C" />
	</conspts>
</geothm>
