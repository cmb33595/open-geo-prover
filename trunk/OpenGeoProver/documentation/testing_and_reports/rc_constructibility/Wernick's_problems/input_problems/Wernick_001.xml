<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE geothm SYSTEM "geothm.dtd">
<geothm name="Wernick 001 (A,B,O --| C)">
	<!-- Wernick 001                                                            -->
	<!-- Let ABC be a triangle and O is the circumcenter.                       -->
	<!-- Consider RC-constructibility of vertex C from given points A, B and O. -->
	<constructions>
		<pfree label="A"/>
		<pfree label="B"/>
		<pfree label="C"/>
		<lperpbis label="mc" point1="A" point2="B" />
		<lperpbis label="ma" point1="B" point2="C" />
		<pintersect label="O" set1="ma" set2="mc" />
	</constructions>
	<freepts>
		<point label="A" />
		<point label="B" />
		<point label="O" />
	</freepts>
	<conspts>
		<point label="C" />
	</conspts>
</geothm>
